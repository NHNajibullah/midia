{
    "name": "nauvalazhar/midia",
    "description": "Simple Media manager for your Laravel project",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filemanager",
        "dropzone",
        "upload",
        "file",
        "manager",
        "image",
        "ckeditor",
        "tinymce",
        "summernote"
    ],
    "authors": [
        {
            "name": "Muhamad Nauval Azhar",
            "email": "nauvalazhar2@gmail.com"
        },
        {
            "name": "Multinity",
            "email": "hi@multinity.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "intervention/image": "2.*"
    },
    "autoload": {
        "classmap": [
            "src"
        ],
        "psr-4": {
            "Nauvalazhar\\Midia\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nauvalazhar\\Midia\\MidiaServiceProvider"
            ]
        }
    }
}
